{"components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       data-search-quick\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                >\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                    <svg>\n                        <use href=\"#icon-{{display_name}}\"/>\n                    </svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/store-logo":"<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n        <img class=\"header-logo-image-unknown-size\"\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\">\n        {{else}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\"\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                 alt=\"{{settings.store_logo.title}}\"\n                 title=\"{{settings.store_logo.title}}\">\n        </div>\n        {{/if}}\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","components/custom/header":"<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n\r\n<header class=\"header\">\r\n    <div class=\"header-container\">\r\n        <!-- menu button and search button -->\r\n        <div class=\"buttons\">\r\n            <button class=\"mobileMenu-toggle\" title=\"main menu navigation\" aria-label=\"main menu navigation\">\r\n                <span class=\"toggle-bar\"></span>\r\n                <span class=\"toggle-bar\"></span>\r\n                <span class=\"toggle-bar\"></span>\r\n            </button>\r\n            <button class=\"navUser-action navUser-action--quickSearch\"\r\n               type=\"button\"\r\n               id=\"quick-search-expand\"\r\n               data-search=\"quickSearch\"\r\n               aria-controls=\"quickSearch\"\r\n               aria-label=\"{{lang 'common.search'}}\"\r\n               title=\"{{lang 'common.search'}} our store\"\r\n            >\r\n            <svg class=\"icon\">\r\n                <use xlink:href=\"#icon-search\" />\r\n            </svg>\r\n            </button>\r\n        </div>\r\n        <!-- store logo -->\r\n        <div class=\"menu-logo\">\r\n            {{> components/common/store-logo}}\r\n        </div>\r\n        <!-- account button and cart button -->\r\n        <div class=\"buttons\">\r\n            <a class=\"navUser-action\"\r\n                   href=\"{{urls.account.index}}\"\r\n                   aria-label=\"My Account\"\r\n                   title=\"{{lang 'common.account'}}\"\r\n                ><button class=\"account-toggle header-button-alt\">\r\n                <svg class=\"icon\">\r\n                    <use xlink:href=\"#icon-user\" />\r\n                </svg>\r\n            </button>\r\n            </a>\r\n            <a class=\"cart-toggle header-button-alt\"\r\n            data-cart-preview\r\n            data-dropdown=\"cart-preview-dropdown\"\r\n            href=\"{{urls.cart}}\"\r\n            aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n            title=\"{{lang 'cart.nav_aria_label'}}\"\r\n            >\r\n                <span id=\"cart-button-qty\" class=\"countPill cart-quantity\"></span>\r\n                <svg class=\"icon\">\r\n                    <use xlink:href=\"#icon-cart\" />\r\n                </svg>\r\n            </a>\r\n\r\n        </div>\r\n    </div>\r\n</header>\r\n\r\n\r\n{{{region name=\"header_bottom--global\"}}}\r\n\r\n{{{region name=\"header_bottom\"}}}\r\n\r\n<div id=\"mobile-menu\" style=\"display: none\">\r\n    {{> components/custom/menu }}\r\n</div>\r\n<div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\r\n    {{> components/common/quick-search name='nav-quick-search'}}\r\n</div>\r\n<div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\" style=\"\"></div>\r\n\r\n<script>\r\n    window.onload = function() {\r\n        const menuBtn = document.querySelector(\".mobileMenu-toggle\");\r\n        const close = document.getElementById(\"close-mobile-menu\");\r\n        const mobileMenu = document.getElementById(\"mobile-menu\");\r\n        const body = document.querySelector(\"body\");\r\n    \r\n        menuBtn.addEventListener(\"click\", function(e) {\r\n            if (mobileMenu.style.display == \"none\") {\r\n                mobileMenu.style.display = \"block\";\r\n                mobileMenu.style.height = `${window.innerHeight}px`;\r\n                body.style.overflow = \"hidden\";\r\n            } else {\r\n                mobileMenu.style.display = \"none\";\r\n                body.style.overflow = \"auto\";\r\n            }\r\n        });\r\n    \r\n        close.addEventListener(\"click\", function() {\r\n            mobileMenu.style.display = \"none\";\r\n            body.style.overflow = \"auto\";\r\n        })\r\n        \r\n        \r\n    } \r\n\r\n    const previewCart = document.getElementById(\"cart-preview-dropdown\");\r\n    const previewCartOptions = {\r\n        attributes: true,\r\n    }\r\n\r\n    function callback(mutationList, observer) {\r\n        mutationList.forEach((mutation) => {\r\n            if (mutation.type === 'attributes' && mutation.attributeName === 'class') {\r\n                if (!previewCart.classList.contains(\"is-open\")) {\r\n                    previewCart.style.display = \"none\";\r\n                }\r\n            }\r\n        })\r\n    }\r\n\r\n    const observer = new MutationObserver(callback);\r\n    observer.observe(previewCart, previewCartOptions);\r\n</script>\r\n","components/custom/menu":"<div class=\"mobile-menu\">\n  <h2>Main Menu</h2>\n  <button id=\"close-mobile-menu\">&#10005;</button>\n  <!-- Shop Products -->\n  <div class=\"nav-categories\">\n    <h3>Shop Products</h3>\n    <div class=\"nav-list\">\n    {{#each categories}}\n      <a href=\"{{url}}\">{{name}}</a>\n    {{/each}}\n    </div>\n  </div>\n\n  {{#each pages}}\n  <div class=\"nav-categories\">\n    <h3>{{name}}</h3>\n            <div class=\"nav-list\">\n                {{#each children}}\n                <a href=\"{{url}}\">{{name}}</a>\n                {{/each}}\n            </div>\n  </div>\n  {{/each}}\n\n  <div class=\"nav-categories\">\n    <h3>Account</h3>\n    <div class=\"nav-list-alt\">\n      <div>\n        {{#if customer}}\n        <a href=\"/logout.php\">Logout</a>\n        {{else}}\n        <a href=\"/login.php\">Login</a>\n        {{/if}}\n        <a href=\"/login.php?action=create_account\">Register</a>\n      </div>\n\n      <div>\n        <a href=\"/account.php\">My Account</a>\n        <a href=\"/account.php?action=order_status\">Order History</a>\n        <a href=\"/account.php?action=address_book\">Addresses</a>\n        <a href=\"#\">Reorder</a>\n      </div>\n    </div>\n  </div>\n  {{> components/common/social-links }}\n</div>\n"}
